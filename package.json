{
  "name": "pnpm-feature-architect-template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint:secretlint": "secretlint '**/*'",
    "lint:ls-lint": "ls-lint",
    "lint:markup-lint": "markuplint 'src/**/*.{jsx,tsx}'",
    "lint:ts-prune": "ts-prune",
    "lint:biome": "biome check --write ./src/**/*",
    "lint:all": "pnpm lint:biome && pnpm typecheck && pnpm lint:markup-lint && pnpm lint:secretlint && pnpm lint:ls-lint && pnpm lint:ts-prune",
    "create:page": "hygen page new",
    "create:feature": "hygen feature new",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest \"src\""
  },
  "dependencies": {
    "@types/node": "22.7.4",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.20",
    "next": "14.2.14",
    "postcss": "8.4.47",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss": "3.4.13",
    "typescript": "5.6.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@ls-lint/ls-lint": "2.2.3",
    "@markuplint/jsx-parser": "4.7.7",
    "@markuplint/react-spec": "4.5.7",
    "@secretlint/secretlint-rule-preset-recommend": "8.3.3",
    "@storybook/addon-essentials": "8.3.5",
    "@storybook/addon-interactions": "8.3.5",
    "@storybook/addon-links": "8.3.5",
    "@storybook/addon-onboarding": "8.3.5",
    "@storybook/blocks": "8.3.5",
    "@storybook/nextjs": "8.3.5",
    "@storybook/react": "8.3.5",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "@vitejs/plugin-react": "4.3.2",
    "axios": "1.7.7",
    "axios-case-converter": "1.1.1",
    "dotenv": "16.4.5",
    "hygen": "6.2.11",
    "jsdom": "25.0.1",
    "lefthook": "1.7.18",
    "markuplint": "4.9.4",
    "msw": "^2.4.9",
    "nookies": "2.5.2",
    "secretlint": "8.3.3",
    "storybook": "8.3.5",
    "swr": "2.2.5",
    "ts-prune": "0.10.3",
    "vitest": "2.1.2",
    "zod": "3.23.8"
  }
}
