{
  "name": "pnpm-feature-architect-template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint:prettier": "prettier --write 'src/**/*.{js,ts,tsx,json}'",
    "lint:eslint": "eslint --ext .js,.ts,.tsx 'src/**/*.{js,ts,tsx}'",
    "lint:secretlint": "secretlint '**/*'",
    "lint:ls-lint": "ls-lint",
    "lint:markup-lint": "markuplint 'src/**/*.{jsx,tsx}'",
    "lint:github-actions": "eslint --ext .js,.ts,.tsx",
    "lint:ts-prune": "ts-prune",
    "lint:all": "pnpm typecheck && pnpm lint:prettier && pnpm lint:eslint && pnpm lint:markup-lint && pnpm lint:secretlint && pnpm lint:ls-lint && pnpm lint:ts-prune",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "create:page": "hygen page new",
    "create:feature": "hygen feature new",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest \"src\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{jsx,js,tsx,ts}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@types/node": "20.14.10",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "next": "14.2.5",
    "postcss": "8.4.39",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "recoil": "0.7.7",
    "tailwindcss": "3.4.4",
    "typescript": "5.5.3"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@ianvs/prettier-plugin-sort-imports": "4.3.1",
    "@ls-lint/ls-lint": "2.2.3",
    "@markuplint/jsx-parser": "4.7.5",
    "@markuplint/react-spec": "4.5.5",
    "@secretlint/secretlint-rule-preset-recommend": "8.2.4",
    "@storybook/addon-essentials": "8.2.2",
    "@storybook/addon-interactions": "8.2.2",
    "@storybook/addon-links": "8.2.2",
    "@storybook/addon-onboarding": "8.2.2",
    "@storybook/blocks": "8.2.2",
    "@storybook/nextjs": "8.2.2",
    "@storybook/react": "8.2.2",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/jest-dom": "6.4.6",
    "@testing-library/react": "16.0.0",
    "@typescript-eslint/eslint-plugin": "7.16.0",
    "@typescript-eslint/parser": "7.16.0",
    "@vitejs/plugin-react": "4.3.1",
    "axios": "1.7.2",
    "axios-case-converter": "1.1.1",
    "dotenv": "16.4.5",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-import-access": "2.1.1",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-storybook": "0.6.13",
    "eslint-plugin-strict-dependencies": "1.2.1",
    "eslint-plugin-tailwindcss": "3.13.0",
    "eslint-plugin-tree-shakable": "1.0.0",
    "eslint-plugin-typescript-sort-keys": "2.3.0",
    "husky": "9.0.11",
    "hygen": "6.2.11",
    "jsdom": "24.1.1",
    "lint-staged": "15.2.7",
    "markuplint": "4.9.2",
    "msw": "^2.3.2",
    "nookies": "2.5.2",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.5",
    "secretlint": "8.2.4",
    "storybook": "8.2.2",
    "swr": "2.2.5",
    "ts-prune": "0.10.3",
    "vitest": "2.0.3",
    "zod": "3.23.8"
  }
}
